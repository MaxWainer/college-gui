<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<AnchorPane prefHeight="600.0" prefWidth="900.0" xmlns="http://javafx.com/javafx/18"
  xmlns:fx="http://javafx.com/fxml/1"
  fx:controller="maxwainer.college.gui.pages.BasePageController">
  <Label fx:id="loggedInLabel" layoutX="29.0" layoutY="27.0" prefHeight="53.0" prefWidth="835.0"
    text="Logged-in as:" textAlignment="CENTER">
    <font>
      <Font size="17.0"/>
    </font>
  </Label>
  <Button layoutX="228.0" layoutY="270.0" mnemonicParsing="false"
    onMouseClicked="#onOrderTicketClick" prefHeight="61.0" prefWidth="142.0" text="Order ticket"/>
  <Button layoutX="379.0" layoutY="270.0" mnemonicParsing="false"
    onMouseClicked="#onListTicketsClick" prefHeight="61.0" prefWidth="142.0"
    text="List my tickets"/>
  <Button layoutX="531.0" layoutY="270.0" mnemonicParsing="false"
    onMouseClicked="#onCheckActivesClick" prefHeight="61.0" prefWidth="142.0" text="Check actives"/>
  <!-- admin/moderator buttons -->
  <Button fx:id="activesEditor" visible="false" layoutX="228.0" layoutY="347.0" mnemonicParsing="false"
    onMouseClicked="#onEditActivesClick" prefHeight="61.0" prefWidth="142.0" text="Edit Actives"/>
  <Button fx:id="trainsEditor" visible="false" layoutX="379.0" layoutY="347.0" mnemonicParsing="false"
    onMouseClicked="#onEditTrainsClick" prefHeight="61.0" prefWidth="142.0" text="Edit Trains"/>
  <Button fx:id="directionsEditor" visible="false" layoutX="531.0" layoutY="347.0" mnemonicParsing="false"
    onMouseClicked="#onEditDirectionsClick" prefHeight="61.0" prefWidth="142.0"
    text="Edit Directions"/>
  <Button fx:id="usersEditor" visible="false" layoutX="379.0" layoutY="420.0" mnemonicParsing="false"
    onMouseClicked="#onEditUsersClick" prefHeight="61.0" prefWidth="142.0" text="Edit Users"/>
</AnchorPane>
