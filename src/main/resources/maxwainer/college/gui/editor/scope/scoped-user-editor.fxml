<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<AnchorPane prefHeight="600.0" prefWidth="900.0" xmlns="http://javafx.com/javafx/18"
  xmlns:fx="http://javafx.com/fxml/1"
  fx:controller="maxwainer.college.gui.pages.editor.admin.ScopedUserEditorPage">
  <TableView fx:id="ticketList" layoutX="186.0" layoutY="14.0" prefHeight="500.0" prefWidth="700.0">
    <columns>
      <TableColumn fx:id="sittingIndexColumn" prefWidth="100.0" text="Sitting Index"/>
      <TableColumn fx:id="sittingTypeColumn" prefWidth="100.0" text="Sitting type"/>
      <TableColumn fx:id="carriageColumn" prefWidth="100.0" text="Carriage"/>
      <TableColumn fx:id="trainColumn" prefWidth="100.0" text="Train"/>
      <TableColumn fx:id="endStationColumn" prefWidth="100.0" text="End station"/>
      <TableColumn fx:id="timeColumn" prefWidth="100.0" text="Time"/>
      <TableColumn fx:id="directionColumn" prefWidth="100.0" text="Direction"/>
    </columns>
  </TableView>
  <Button layoutX="768.0" layoutY="561.0" mnemonicParsing="false"
    onMouseClicked="#onBackButtonClick" text="Back to main menu" />
  <Button layoutX="618.0" layoutY="561.0" mnemonicParsing="false"
    onMouseClicked="#onRemoveSelectedItems" text="Remove selected items"/>
  <Button layoutX="14.0" layoutY="561.0" mnemonicParsing="false" onMouseClicked="#onSaveChangesClick"
    text="Save changes"/>
  <ComboBox fx:id="rolesBox" layoutX="14.0" layoutY="93.0" prefWidth="150.0" promptText="Choose new role"/>
</AnchorPane>
