  <TextField layoutX="377.0" layoutY="235.0" promptText="Enter username..." fx:id="usernameField"/>
  <NumericField layoutX="377.0" layoutY="197.0" promptText="Enter passport id..."
    fx:id="passportIdField"/>
</AnchorPane>
